@model WebApplication1.DTO.CategoryDTO

@{
    ViewData["Title"] = "Category Details";
}

<h2>Category Details</h2>

@if (Model != null)
{
    <table border="1">
        <thead>
            <tr>
                <th>Category Name</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@Model.CategoryName</td>
            </tr>
        </tbody>
    </table>
}
else
{
    <p>No category found.</p>
}